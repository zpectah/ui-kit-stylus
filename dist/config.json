{
  "import": {
    "root": true,
    "reset": true,
    "body": true
  },
  "base": {
    "spacer": "1rem",
    "link": "rgb(123,31,162)",
    "font": {
      "family": "system-ui, -apple-system, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, \"Noto Sans\", \"Liberation Sans\", sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\"",
      "size": "16px",
      "weight": 400,
      "line-height": 1.5
    },
    "border": {
      "width": "1px",
      "style": "solid",
      "radius": ".25rem"
    },
    "transition": {
      "duration": ".25s",
      "delay": "0s"
    }
  },
  "prefix": {
    "global": "",
    "variable": "kit"
  },
  "suffix": {
    "print": "print",
    "first": "first",
    "last": "last",
    "focusable": "focusable",
    "negative": "n",
    "top": "t",
    "right": "r",
    "bottom": "b",
    "left": "l",
    "x": "x",
    "y": "y",
    "horizontal": "h",
    "vertical": "v"
  },
  "breakpoints": {
    "xs": 0,
    "sm": "576px",
    "md": "768px",
    "lg": "992px",
    "xl": "1200px",
    "xxl": "1400px"
  },
  "palette": {
    "default": "rgb(69,90,100)",
    "primary": "rgb(104,104,172)",
    "secondary": "rgb(98,93,96)",
    "success": "rgb(56,142,60)",
    "info": "rgb(2,136,209)",
    "warning": "rgb(245,124,0)",
    "error": "rgb(211,47,47)",
    "light": "rgb(250,250,250)",
    "dark": "rgb(41,40,45)"
  },
  "utilities": {
    "enabled": true,
    "important": true,
    "vertical-align": {
      "enabled": true,
      "property": "vertical-align",
      "class": "vertical-align",
      "values": "baseline sub super top text-top middle bottom text-bottom inherit"
    },
    "background": {
      "enabled": true,
      "property": "background",
      "class": "bg",
      "values": "none inherit",
      "list_min": 1,
      "list_max": 9,
      "value_counter": 10
    },
    "color": {
      "enabled": true,
      "property": "color",
      "class": "color",
      "values": "inherit",
      "list_min": 1,
      "list_max": 9,
      "value_counter": 10
    },
    "display": {
      "enabled": true,
      "responsive": true,
      "print": true,
      "property": "display",
      "class": "d",
      "values": "inherit none inline inline-block block grid table table-cell table-row flex inline-flex"
    },
    "flex": {
      "enabled": true,
      "flex": {
        "responsive": true,
        "property": "flex",
        "class": "flex",
        "values": "auto none inherit 1 0"
      },
      "direction": {
        "responsive": true,
        "property": "flex-direction",
        "class": "flex-direction",
        "values": "row row-reverse column column-reverse inherit"
      },
      "justify-content": {
        "responsive": true,
        "property": "justify-content",
        "class": "justify-content",
        "values": "flex-start flex-end center space-between space-around space-evenly inherit"
      },
      "justify-items": {
        "responsive": true,
        "property": "justify-items",
        "class": "justify-items",
        "values": "flex-start flex-end center inherit"
      },
      "justify-self": {
        "responsive": true,
        "property": "justify-self",
        "class": "justify-self",
        "values": "auto normal stretch center flex-start flex-end self-start self-end left right baseline inherit revert unset"
      },
      "align-content": {
        "responsive": true,
        "property": "align-content",
        "class": "align-content",
        "values": "stretch center flex-start flex-end space-between space-around inherit"
      },
      "align-items": {
        "responsive": true,
        "property": "align-items",
        "class": "align-items",
        "values": "stretch center flex-start flex-end baseline inherit"
      },
      "align-self": {
        "responsive": true,
        "property": "align-self",
        "class": "align-self",
        "values": "auto stretch center flex-start flex-end baseline inherit"
      },
      "fill": {
        "responsive": true,
        "property": "flex",
        "class": "flex-fill",
        "value": "1 1 auto"
      },
      "grow": {
        "responsive": true,
        "property": "flex-grow",
        "class": "flex-grow",
        "values": "inherit",
        "list_min": 0,
        "list_max": 5
      },
      "shrink": {
        "responsive": true,
        "property": "flex-shrink",
        "class": "flex-shrink",
        "values": "inherit",
        "list_min": 0,
        "list_max": 5
      },
      "wrap": {
        "responsive": true,
        "property": "flex-wrap",
        "class": "flex-wrap",
        "values": "nowrap wrap wrap-reverse inherit"
      },
      "order": {
        "responsive": true,
        "property": "order",
        "class": "flex-order",
        "list_min": 0,
        "list_max": 5
      },
      "gap": {
        "responsive": true,
        "property": "gap",
        "class": "gap",
        "list_min": 0,
        "list_max": 5
      }
    },
    "float": {
      "enabled": true,
      "responsive": true,
      "property": "float",
      "class": "float",
      "values": "none left right inherit"
    },
    "spacing": {
      "enabled": true,
      "responsive": true,
      "list_min": 0,
      "list_max": 5,
      "list_modulo": 1,
      "margin": {
        "responsive": true,
        "property": "margin",
        "class": "m",
        "values": "auto inherit",
        "listNeg_min": 1,
        "listNeg_max": 5
      },
      "padding": {
        "responsive": true,
        "property": "padding",
        "class": "p",
        "values": "inherit"
      }
    },
    "sizing": {
      "enabled": true,
      "responsive": true,
      "list_min": 0,
      "list_max": 100,
      "list_modulo": 5,
      "width": {
        "responsive": true,
        "property": "width",
        "class": "w",
        "values": "auto inherit"
      },
      "min-width": {
        "responsive": true,
        "property": "min-width",
        "class": "w-min",
        "values": "inherit"
      },
      "max-width": {
        "responsive": true,
        "property": "max-width",
        "class": "w-max",
        "values": "none inherit"
      },
      "height": {
        "responsive": true,
        "property": "height",
        "class": "h",
        "values": "auto inherit"
      },
      "min-height": {
        "responsive": true,
        "property": "min-height",
        "class": "h-min",
        "values": "inherit"
      },
      "max-height": {
        "responsive": true,
        "property": "max-height",
        "class": "h-max",
        "values": "none inherit"
      },
      "viewport-width": {
        "responsive": true,
        "property": "width",
        "class": "vw",
        "values": "auto inherit"
      },
      "min-viewport-width": {
        "responsive": true,
        "property": "min-width",
        "class": "vw-min",
        "values": "inherit"
      },
      "viewport-height": {
        "responsive": true,
        "property": "height",
        "class": "vh",
        "values": "auto inherit"
      },
      "min-viewport-height": {
        "responsive": true,
        "property": "min-height",
        "class": "vh-min",
        "values": "inherit"
      }
    },
    "opacity": {
      "enabled": true,
      "property": "opacity",
      "class": "opacity",
      "list_min": 0,
      "list_max": 100,
      "list_modulo": 5,
      "value_counter": 100
    },
    "text": {
      "enabled": true,
      "align": {
        "responsive": true,
        "property": "text-align",
        "class": "text-align",
        "values": "left right center justify inherit"
      },
      "decoration": {
        "property": "text-decoration",
        "class": "text-decoration",
        "values": "underline overline line-through none inherit"
      },
      "justify": {
        "property": "text-justify",
        "class": "text-justify",
        "values": "auto inter-word inter-character none inherit"
      },
      "overflow": {
        "property": "text-overflow",
        "class": "text-overflow",
        "values": "clip ellipsis inherit"
      },
      "transform": {
        "property": "text-transform",
        "class": "text-transform",
        "values": "none capitalize uppercase lowercase inherit"
      },
      "white-space": {
        "property": "white-space",
        "class": "white-space",
        "values": "normal nowrap pre pre-line pre-wrap inherit"
      },
      "word-break": {
        "property": "word-break",
        "class": "word-break",
        "values": "normal break-all keep-all break-word inherit"
      },
      "word-wrap": {
        "property": "word-wrap",
        "class": "word-wrap",
        "values": "normal break-word inherit"
      }
    },
    "visibility": {
      "enabled": true,
      "responsive": true,
      "property": "visibility",
      "class": "visibility",
      "values": "visible hidden collapse inherit"
    }
  },
  "helpers": {
    "enabled": true,
    "visually-hidden": {
      "enabled": true,
      "class": "visually-hidden",
      "responsive": true
    },
    "clearfix": {
      "enabled": true,
      "class": "clearfix"
    },
    "stack": {
      "enabled": true,
      "class": "stack"
    },
    "text": {
      "enabled": true,
      "truncate": {
        "class": "text-truncate"
      }
    }
  },
  "ui": {
    "enabled": true,
    "container": {
      "enabled": true,
      "class": "container",
      "max-width": {
        "xs": "100%",
        "sm": "540px",
        "md": "720px",
        "lg": "960px",
        "xl": "1140px",
        "xxl": "1320px"
      }
    },
    "typography": {
      "enabled": true,
      "heading": {
        "list": "h1 h2 h3 h4 h5 h6"
      }
    },
    "button": {
      "enabled": true,
      "class": "btn",
      "active-shadow": "0 0 .025rem .125rem",
      "palette": {
        "enabled": true,
        "outline": {
          "enabled": true,
          "prefix": "outline"
        }
      },
      "size": {
        "enabled": true,
        "default": {
          "height": "2.375rem",
          "padding": "0 .85rem",
          "font-size": "1rem",
          "line-height": 1.05
        },
        "small": {
          "prefix": "sm",
          "height": "2rem",
          "padding": "0 .5rem",
          "font-size": "0.85rem",
          "line-height": 1.05
        },
        "large": {
          "prefix": "lg",
          "height": "2.75rem",
          "padding": "0 1rem",
          "font-size": "1.125rem",
          "line-height": 1.05
        }
      }
    },
    "chip": {
      "enabled": true,
      "class": "chip",
      "active-shadow": "0 0 .0125rem .05rem",
      "palette": {
        "enabled": true,
        "outline": {
          "enabled": true,
          "prefix": "outline"
        }
      },
      "size": {
        "enabled": true,
        "default": {
          "height": "1.5rem",
          "padding": "0 .5rem",
          "font-size": ".85rem",
          "line-height": 1.05
        },
        "small": {
          "prefix": "sm",
          "height": "1.25rem",
          "padding": "0 .4rem",
          "font-size": "0.7rem",
          "line-height": 1.05
        },
        "large": {
          "prefix": "lg",
          "height": "1.75rem",
          "padding": "0 .7rem",
          "font-size": ".9rem",
          "line-height": 1.05
        }
      }
    },
    "card": {
      "enabled": true,
      "class": "card"
    },
    "divider": {
      "enabled": true,
      "class": "divider"
    },
    "input": {
      "enabled": true,
      "class": "input"
    },
    "menu": {
      "enabled": true,
      "class": "menu"
    }
  }
}