for prop in (props-sizing-width-values)
  .{prefix-global}{props-sizing-width-class}-{prop}
    width-scope(prop)

for prop in (props-sizing-height-values)
  .{prefix-global}{props-sizing-height-class}-{prop}
    height-scope(prop)

for prop in (props-sizing-viewport-width-values)
  .{prefix-global}{props-sizing-viewport-width-class}-{prop}
    width-scope(prop)

for prop in (props-sizing-viewport-height-values)
  .{prefix-global}{props-sizing-viewport-height-class}-{prop}
    height-scope(prop)

for prop in (props-sizing-min-width-values)
  .{prefix-global}{props-sizing-min-width-class}-{prop}
    min-width-scope(prop)

for prop in (props-sizing-min-height-values)
  .{prefix-global}{props-sizing-min-height-class}-{prop}
    min-height-scope(prop)

for prop in (props-sizing-max-width-values)
  .{prefix-global}{props-sizing-max-width-class}-{prop}
    max-width-scope(prop)

for prop in (props-sizing-max-height-values)
  .{prefix-global}{props-sizing-max-height-class}-{prop}
    max-height-scope(prop)

for prop in (props-sizing-values)
  .{prefix-global}{props-sizing-width-class}-{prop}
    width-scope(prop, '%')

  .{prefix-global}{props-sizing-height-class}-{prop}
    height-scope(prop, '%')

  .{prefix-global}{props-sizing-viewport-width-class}-{prop}
    width-scope(prop, 'vw')

  .{prefix-global}{props-sizing-viewport-height-class}-{prop}
    height-scope(prop, 'vh')

  .{prefix-global}{props-sizing-min-width-class}-{prop}
    min-width-scope(prop, '%')

  .{prefix-global}{props-sizing-min-height-class}-{prop}
    min-height-scope(prop, '%')

  .{prefix-global}{props-sizing-max-width-class}-{prop}
    max-width-scope(prop, '%')

  .{prefix-global}{props-sizing-max-height-class}-{prop}
    max-height-scope(prop, '%')

  .{prefix-global}{props-sizing-min-viewport-width-class}-{prop}
    min-width-scope(prop, 'vw')

  .{prefix-global}{props-sizing-min-viewport-height-class}-{prop}
    min-height-scope(prop, 'vh')

  .{prefix-global}{props-sizing-max-viewport-width-class}-{prop}
    max-width-scope(prop, 'vw')

  .{prefix-global}{props-sizing-max-viewport-height-class}-{prop}
    max-height-scope(prop, 'vh')
