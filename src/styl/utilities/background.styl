for prop in (props-background-values)
  .{prefix-global}{props-background-class}-{prop}
    background-scope(prop)

for prop, value in palette
  .{prefix-global}{props-background-class}-{prop}
    background-scope(value, num)

  for num in (props-background-counter_min..props-background-counter_max)
    .{prefix-global}{props-background-class}-{prop}-{num}
      background-scope(value, num)
