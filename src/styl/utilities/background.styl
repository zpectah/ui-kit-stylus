for prop in (utilities-background-values)
  .{prefix-global}{utilities-background-class}-{prop}
    background-scope(prop)

for prop, value in palette
  .{prefix-global}{utilities-background-class}-{prop}
    background-scope(value, num)

  for num in (utilities-background-counter_min..utilities-background-counter_max)
    .{prefix-global}{utilities-background-class}-{prop}-{num}
      background-scope(value, num)
