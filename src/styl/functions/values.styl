getRemValue(value)
  if value > 0
    val = value * spacer
  else
    val = value

  val


getNegativeValue(value)
  (- value)


getWidthValue(value, unit = '')
  if (value > 0)
    if (unit == '%')
      val = value + unit
    else if (unit == 'vw')
      val = value * 1vw
  else
    val = value

  val


getHeightValue(value, unit = '')
  if (value > 0)
    if (unit == '%')
      val = value + unit
    else if (unit == 'vh')
      val = value * 1vh
  else
    val = value

  val


getUtilitiesImportantSuffix()
  if utilities-important
    !important

